{
  "type": "module",
  "name": "tbd",
  "version": "0.0.0",
  "main": "src/index.ts",
  "author": "tbd",
  "private": true,
  "scripts": {
    "psy": "NODE_ENV=${NODE_ENV:-test} tsx src/conf/system/cli.ts",
    "psy:js": "node ./dist/src/conf/system/cli.js",
    "build": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version echo \"building $npm_package_name...\" && rm -rf dist && echo \"node --version\" && node --version && echo \"npx tsc --version\" && npx tsc --version && npx tsc -p ./tsconfig.build.json",
    "build:spec": "NODE_ENV=test APP_NAME=$npm_package_name APP_VERSION=$npm_package_version echo \"building $npm_package_name...\" && rm -rf dist && echo \"node --version\" && node --version && echo \"npx tsc --version\" && npx tsc --version && npx tsc --pretty -p ./tsconfig.build-spec.json && cp -r ./recordings ./dist/recordings && [ -e .env.test ] && cp .env.test ./dist/",
    "dev": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=development WORKER_COUNT=0 tsx ./src/main.ts",
    "dev-worker": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=development WORKER_COUNT=1 tsx ./src/worker.ts",
    "prod": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=production node ./dist/src/main.js",
    "worker": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=production node ./dist/src/worker.js",
    "sync": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version yarn psy sync:all",
    "uspec": "yarn psy db:integrity-check && APP_NAME=$npm_package_name APP_VERSION=$npm_package_version vitest --config ./spec/unit/vite.config.ts",
    "uspec:js": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version <YARN_RUN> build:spec && cd ./dist && vitest --config ./spec/unit/vite.config.js",
    "fspec": "yarn psy db:integrity-check && APP_NAME=$npm_package_name APP_VERSION=$npm_package_version vitest --config ./spec/features/vite.config.ts",
    "fspec:js": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version <YARN_RUN> build:spec && cd ./dist && vitest --config ./spec/features/vite.config.js",
    "fspec:visible": "yarn psy db:integrity-check && HEADLESS=0 vitest run --config=./spec/features/vite.config.ts",
    "console": "yarn psy db:integrity-check && APP_NAME=$npm_package_name APP_VERSION=$npm_package_version tsx ./src/conf/repl.ts",
    "console:js": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version node ./dist/src/conf/repl.js",
    "format": "<YARN_RUN> prettier . --write",
    "lint": "<YARN_RUN> eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" && <YARN_RUN> prettier . --check"
  },
  "dependencies": {
    "@bull-board/express": "^5.21.4",
    "@rvoh/dream": "^1.7.3",
    "@rvoh/psychic": "^1.10.0",
    "@rvoh/psychic-websockets": "^0.4.0",
    "@rvoh/psychic-workers": "^1.2.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "bullmq": "^5.49.1",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-winston": "^4.2.0",
    "ioredis": "^5.4.1",
    "kysely": "^0.27.4",
    "pg": "^8.12.0",
    "pluralize-esm": "^9.0.5",
    "socket.io": "^4.8.0",
    "socket.io-adapter": "^2.5.5",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@pollyjs/adapter-fetch": "^6.0.6",
    "@pollyjs/adapter-node-http": "^6.0.6",
    "@pollyjs/core": "^6.0.6",
    "@pollyjs/persister-fs": "^6.0.6",
    "@rvoh/dream-spec-helpers": "^1.2.1",
    "@rvoh/psychic-spec-helpers": "^1.1.3",
    "@types/express": "^5.0.1",
    "@types/node": "^22.5.2",
    "@types/pg": "^8.11.8",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.9.1",
    "expect-playwright": "^0.8.0",
    "kysely-codegen": "~0.17.0",
    "luxon-jest-matchers": "^0.1.14",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.3.3",
    "puppeteer": "~24.6.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vitest": "^3.1.1"
  }
}
