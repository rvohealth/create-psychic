{
  "type": "module",
  "name": "tbd",
  "version": "0.0.0",
  "main": "src/index.ts",
  "author": "tbd",
  "private": true,
  "scripts": {
    "psy": "NODE_ENV=${NODE_ENV:-test} tsx src/cli/index.ts",
    "build": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version echo \"building $npm_package_name...\" && rm -rf dist && echo \"node --version\" && node --version && echo \"npx tsc --version\" && npx tsc --version && npx tsc -p ./tsconfig.build.json",
    "dev": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=development WORKER_COUNT=0 tsx ./src/main.ts",
    "dev-worker": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=development WORKER_COUNT=1 tsx ./src/worker.ts",
    "prod": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=production node ./dist/src/main.js",
    "worker": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=production node ./dist/src/worker.js",
    "sync": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version yarn psy sync:all",
    "uspec": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version vitest --config ./spec/unit/vite.config.ts",
    "fspec": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version vitest run --config=./spec/features/vite.config.ts",
    "fspec:visible": "HEADLESS=0 vitest run --config=./spec/features/vite.config.ts",
    "console": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version tsx ./src/conf/repl.ts",
    "format": "<YARN_RUN> prettier . --write",
    "lint": "<YARN_RUN> eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" && <YARN_RUN> prettier . --check"
  },
  "dependencies": {
    "@bull-board/express": "^5.21.4",
    "@rvoh/dream": "^0.32.0",
    "@rvoh/psychic": "^0.26.2",
    "@rvoh/psychic-websockets": "^0.2.3",
    "@rvoh/psychic-workers": "^0.2.6",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "bullmq": "^5.12.12",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-openapi-validator": "^5.3.7",
    "express-winston": "^4.2.0",
    "ioredis": "^5.4.1",
    "kysely": "^0.27.4",
    "pg": "^8.12.0",
    "pluralize-esm": "^9.0.5",
    "socket.io": "^4.8.0",
    "socket.io-adapter": "^2.5.5",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@pollyjs/adapter-fetch": "^6.0.6",
    "@pollyjs/adapter-node-http": "^6.0.6",
    "@pollyjs/core": "^6.0.6",
    "@pollyjs/persister-fs": "^6.0.6",
    "@rvoh/dream-spec-helpers": "^0.2.0",
    "@rvoh/psychic-spec-helpers": "^0.4.1",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.2",
    "@types/pg": "^8.11.8",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.9.1",
    "expect-playwright": "^0.8.0",
    "kysely-codegen": "~0.17.0",
    "luxon-jest-matchers": "^0.1.14",
    "prettier": "^3.3.3",
    "puppeteer": "^24.4.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vitest": "^3.0.8"
  }
}
