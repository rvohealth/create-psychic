{
  "type": "module",
  "name": "tbd",
  "version": "0.0.0",
  "main": "src/index.ts",
  "author": "tbd",
  "private": true,
  "scripts": {
    "client": "yarn --cwd=../client dev",
    "client:fspec": "VITE_PSYCHIC_ENV=test yarn --cwd=../client dev",
    "psy": "node --experimental-specifier-resolution=node --import ./bin/esm.js src/cli/index.ts",
    "build": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version echo \"building $npm_package_name...\" && rm -rf dist && echo \"node --version\" && node --version && echo \"npx tsc --version\" && npx tsc --version && npx tsc -p ./tsconfig.build.json",
    "dev": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=development WORKER_COUNT=0 node --experimental-specifier-resolution=node --import ./bin/esm.js ./src/main.ts",
    "dev-worker": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=development WORKER_COUNT=1 node --experimental-specifier-resolution=node --import ./bin/esm.js ./src/worker.ts",
    "prod": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=production node ./dist/src/main.js",
    "worker": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version NODE_ENV=production node ./dist/src/worker.js",
    "sync": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version yarn psy sync:all",
    "uspec": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version vitest --config ./spec/unit/vite.config.ts",
    "fspec": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version vitest run --config=./spec/features/vite.config.ts",
    "fspec:visible": "HEADLESS=0 vitest run --config=./spec/features/vite.config.ts",
    "console": "APP_NAME=$npm_package_name APP_VERSION=$npm_package_version node --experimental-specifier-resolution=node --import ./bin/esm.js ./src/conf/repl.ts",
    "format": "yarn run prettier . --write",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" && yarn run prettier . --check"
  },
  "dependencies": {
    "@bull-board/express": "^5.21.4",
    "@rvoh/dream": "^0.29.10",
    "@rvoh/psychic": "^0.24.4",
    "@rvoh/psychic-websockets": "^0.2.1",
    "@rvoh/psychic-workers": "^0.2.2",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "bullmq": "^5.12.12",
    "commander": "^12.1.0",
    "express": "^4.21.1",
    "express-openapi-validator": "^5.3.7",
    "express-winston": "^4.2.0",
    "ioredis": "^5.4.1",
    "kysely": "^0.27.4",
    "luxon": "^3.5.0",
    "pg": "^8.12.0",
    "socket.io": "^4.8.0",
    "socket.io-adapter": "^2.5.5",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@rvoh/dream-spec-helpers": "^0.2.0",
    "@rvoh/psychic-spec-helpers": "^0.3.1",
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.2",
    "@types/pg": "^8.11.8",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.9.1",
    "expect-playwright": "^0.8.0",
    "kysely-codegen": "~0.17.0",
    "luxon-jest-matchers": "^0.1.14",
    "prettier": "^3.3.3",
    "puppeteer": "^24.4.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vitest": "^3.0.8"
  },
  "packageManager": "yarn@4.4.1"
}
